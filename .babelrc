{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": "3",
        "useBuiltIns": "entry"
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-syntax-dynamic-import"
      ]
    }
  }
}